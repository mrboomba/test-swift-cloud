// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-arm64-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Song {
  Song    String @unique
  Artist String
  Writer  String
  Album String
  Year Int
  PlaysJune Int
  PlaysJuly Int
  PlaysAugust Int
}